        app:layout_constraintEnd_toEndOf="@+id/imageView77"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/imageView77"
        app:layout_constraintTop_toTopOf="@+id/imageView77"
        app:layout_constraintVertical_bias="0.318" />

    <TextView
        android:id="@+id/textView64"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_medium"
        android:text="Local Shipping"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView77"
        app:layout_constraintEnd_toEndOf="@+id/imageView77"
        app:layout_constraintHorizontal_bias="0.051"
        app:layout_constraintStart_toEndOf="@+id/imageView78"
        app:layout_constraintTop_toTopOf="@+id/imageView77"
        app:layout_constraintVertical_bias="0.358" />

    <TextView
        android:id="@+id/date_delivery"
        android:layout_width="200dp"
        android:layout_height="15dp"
        android:fontFamily="@font/jost_medium"
        android:textAlignment="textStart"
        android:textColor="@color/grey"
        android:textSize="9dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView77"
        app:layout_constraintEnd_toEndOf="@+id/textView64"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView78"
        app:layout_constraintTop_toBottomOf="@+id/textView64" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_medium"
        android:text="â‚±45"
        android:textAlignment="center"
        android:textColor="@color/C35314"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView77"
        app:layout_constraintEnd_toEndOf="@+id/imageView77"
        app:layout_constraintHorizontal_bias="0.835"
        app:layout_constraintStart_toEndOf="@+id/textView64"
        app:layout_constraintTop_toTopOf="@+id/imageView77"
        app:layout_constraintVertical_bias="0.487" />

</androidx.constraintlayout.widget.ConstraintLayout>