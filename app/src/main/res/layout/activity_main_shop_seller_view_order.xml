<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".Main_Shop_Seller_View_Order">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@color/darkbrown"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button3"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/vector"
        android:onClick="my_shop_view_orders_back_btn"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.687"
        tools:ignore="OnClick" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:fontFamily="@font/koho_semibold"
        android:text="Order Details"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.833"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView97"
        android:layout_width="340dp"
        android:layout_height="113dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/order_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/picture_product_detail"
        android:layout_width="57dp"
        android:layout_height="55dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView97"
        app:layout_constraintEnd_toEndOf="@+id/imageView97"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/imageView97"
        app:layout_constraintTop_toTopOf="@+id/imageView97" />

    <TextView
        android:id="@+id/name_product_detail"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView97"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toEndOf="@+id/picture_product_detail"
        app:layout_constraintTop_toTopOf="@+id/imageView97"
        app:layout_constraintVertical_bias="0.172" />


    <TextView
        android:id="@+id/quantity_product_detail"
        android:layout_width="40dp"
        android:layout_height="15dp"
        android:fontFamily="@font/jost_regular"
        android:text="Quantity:"
        android:textAlignment="textStart"
        android:textColor="@color/grey"
        android:textSize="9dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView97"
        app:layout_constraintEnd_toEndOf="@+id/name_product_detail"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toEndOf="@+id/picture_product_detail"
        app:layout_constraintTop_toBottomOf="@+id/name_product_detail"
        app:layout_constraintVertical_bias="0.129" />

    <TextView
        android:id="@+id/quantity_amount_product_detail"
        android:layout_width="40dp"
        android:layout_height="15dp"
        android:fontFamily="@font/jost_regular"
        android:textAlignment="textStart"
        android:textColor="@color/grey"
        android:textSize="9dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView97"
        app:layout_constraintEnd_toEndOf="@+id/imageView97"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/quantity_product_detail"
        app:layout_constraintTop_toBottomOf="@+id/name_product_detail"
        app:layout_constraintVertical_bias="0.1" />


    <TextView
        android:id="@+id/price_product_detail"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:fontFamily="@font/jost_medium"
        android:textAlignment="textStart"
        android:textColor="@color/C35314"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView97"
        app:layout_constraintEnd_toEndOf="@+id/imageView97"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toEndOf="@+id/picture_product_detail"
        app:layout_constraintTop_toBottomOf="@+id/quantity_product_detail"
        app:layout_constraintVertical_bias="0.216" />

    <ImageView
        android:id="@+id/address_view"
        android:layout_width="340dp"
        android:layout_height="170dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/order_background"
        app:layout_constraintBottom_toTopOf="@+id/imageView85"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView97"
        app:layout_constraintVertical_bias="0.338" />

    <TextView
        android:id="@+id/textView74"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_medium"
        android:text="Shipping Details"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/address_view"
        app:layout_constraintEnd_toEndOf="@+id/address_view"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/address_view"
        app:layout_constraintTop_toTopOf="@+id/address_view"
        app:layout_constraintVertical_bias="0.042" />


    <TextView
        android:id="@+id/label_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jost_semibold"
        android:paddingLeft="10dp"
        android:text="Label"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/address_view"
        app:layout_constraintEnd_toEndOf="@+id/address_view"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toStartOf="@+id/address_view"
        app:layout_constraintTop_toBottomOf="@+id/textView74"
        app:layout_constraintVertical_bias="0.131" />


    <TextView
        android:id="@+id/phone_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/jost_regular"
        android:paddingLeft="10dp"
        android:text="Phone"
        android:textAlignment="textStart"
        android:textColor="@color/C35314"
        app:layout_constraintEnd_toEndOf="@+id/address_view"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toStartOf="@+id/address_view"
        app:layout_constraintTop_toBottomOf="@+id/name_detail" />

    <TextView
        android:id="@+id/fullAddress_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jost_semibold"
        android:paddingLeft="10dp"
        android:text="Full Address"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/phone_detail"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/phone_detail"
        app:layout_constraintTop_toBottomOf="@+id/phone_detail"
        app:layout_constraintVertical_bias="0.022" />

    <TextView
        android:id="@+id/name_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/jost_semibold"
        android:paddingLeft="10dp"
        android:text="Name"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/address_view"
        app:layout_constraintEnd_toEndOf="@+id/address_view"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="@+id/address_view"
        app:layout_constraintTop_toBottomOf="@+id/textView74"
        app:layout_constraintVertical_bias="0.377" />

    <ImageView
        android:id="@+id/imageView85"
        android:layout_width="370dp"
        android:layout_height="190dp"
        android:background="@drawable/order_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView97"
        app:layout_constraintVertical_bias="0.711" />

    <TextView
        android:id="@+id/textView68"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_bold"
        android:text="Courier"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView85"
        app:layout_constraintEnd_toEndOf="@+id/imageView85"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toStartOf="@+id/imageView85"
        app:layout_constraintTop_toTopOf="@+id/imageView85"
        app:layout_constraintVertical_bias="0.082" />

    <EditText
        android:id="@+id/courier"
        android:layout_width="318dp"
        android:layout_height="33dp"
        android:background="@drawable/email_reset"
        android:fontFamily="@font/jost_medium"
        android:paddingLeft="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView85"
        app:layout_constraintEnd_toEndOf="@+id/imageView85"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/imageView85"
        app:layout_constraintTop_toTopOf="@+id/imageView85"
        app:layout_constraintVertical_bias="0.273" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_bold"
        android:text="Tracking Number"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView85"
        app:layout_constraintEnd_toEndOf="@+id/imageView85"
        app:layout_constraintHorizontal_bias="0.094"
        app:layout_constraintStart_toStartOf="@+id/imageView85"
        app:layout_constraintTop_toTopOf="@+id/imageView85"
        app:layout_constraintVertical_bias="0.511" />

    <EditText
        android:id="@+id/tracking_number"
        android:layout_width="318dp"
        android:layout_height="33dp"
        android:background="@drawable/email_reset"
        android:fontFamily="@font/jost_medium"
        android:paddingLeft="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView85"
        app:layout_constraintEnd_toEndOf="@+id/imageView85"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/imageView85"
        app:layout_constraintTop_toTopOf="@+id/imageView85"
        app:layout_constraintVertical_bias="0.77" />

    <Button
        android:id="@+id/done_order_detail"
        android:layout_width="120dp"
        android:layout_height="31dp"
        android:background="@drawable/btn"
        android:fontFamily="@font/koho_medium"
        android:text="Done"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView85"
        app:layout_constraintVertical_bias="0.187" />
</androidx.constraintlayout.widget.ConstraintLayout>