<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="150dp">


    <ImageView
        android:id="@+id/imageView88"
        android:layout_width="370dp"
        android:layout_height="86dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.14" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/picture_product_to_receive"
        android:layout_width="67dp"
        android:layout_height="59dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView88"
        app:layout_constraintEnd_toEndOf="@+id/imageView88"
        app:layout_constraintHorizontal_bias="0.029"
        app:layout_constraintStart_toStartOf="@+id/imageView88"
        app:layout_constraintTop_toTopOf="@+id/imageView88"
        app:layout_constraintVertical_bias="0.518"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView71"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:fontFamily="@font/koho_medium"
        android:text="Received by"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView88"
        app:layout_constraintEnd_toEndOf="@+id/imageView88"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toEndOf="@+id/picture_product_to_receive"
        app:layout_constraintTop_toTopOf="@+id/imageView88"
        app:layout_constraintVertical_bias="0.166" />


    <TextView
        android:id="@+id/date_of_receive"
        android:layout_width="150dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="textStart"
        android:textColor="@color/C35314"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView88"
        app:layout_constraintEnd_toEndOf="@+id/imageView88"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView71"
        app:layout_constraintTop_toTopOf="@+id/imageView88"
        app:layout_constraintVertical_bias="0.166" />

    <ImageView
        android:id="@+id/imageView89"
        android:layout_width="370dp"
        android:layout_height="28dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView88"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView88"
        app:layout_constraintTop_toBottomOf="@+id/imageView88"
        app:layout_constraintVertical_bias="0.114" />

    <TextView
        android:id="@+id/textView69"
        android:layout_width="100dp"
        android:layout_height="17dp"
        android:fontFamily="@font/koho_medium"
        android:text="Tracking Number"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView89"
        app:layout_constraintEnd_toEndOf="@+id/imageView89"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/imageView89"
        app:layout_constraintTop_toTopOf="@+id/imageView89"
        app:layout_constraintVertical_bias="0.454" />

    <TextView
        android:id="@+id/tracking_number_to_receive"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="textEnd"
        android:textColor="@color/C35314"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView89"
        app:layout_constraintEnd_toEndOf="@+id/imageView89"
        app:layout_constraintHorizontal_bias="0.982"
        app:layout_constraintStart_toEndOf="@+id/textView69"
        app:layout_constraintTop_toTopOf="@+id/imageView89"
        app:layout_constraintVertical_bias="0.454" />
</androidx.constraintlayout.widget.ConstraintLayout>