<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".User_View_Loading_Screen">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="409dp"
        android:layout_height="230dp"
        android:background="@drawable/lako_header"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="7dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="@color/darkbrown"
        tools:ignore="MissingConstraints"/>

    <ImageView
        android:id="@+id/imageView81"
        android:layout_width="341dp"
        android:layout_height="214dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.05" />

    <ImageView
        android:id="@+id/imageView82"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@drawable/check_shop"
        app:layout_constraintBottom_toBottomOf="@+id/imageView81"
        app:layout_constraintEnd_toEndOf="@+id/imageView81"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView81"
        app:layout_constraintTop_toTopOf="@+id/imageView81"
        app:layout_constraintVertical_bias="0.193" />

    <TextView
        android:id="@+id/textView66"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_medium"
        android:text="Order Placed!"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView81"
        app:layout_constraintEnd_toEndOf="@+id/imageView81"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/imageView81"
        app:layout_constraintTop_toBottomOf="@+id/imageView82"
        app:layout_constraintVertical_bias="0.328" />

    <TextView
        android:id="@+id/date_placed_order"
        android:layout_width="200dp"
        android:layout_height="22dp"
        android:fontFamily="@font/jost_regular"
        android:textAlignment="center"
        android:textColor="@color/C35314"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView81"
        app:layout_constraintEnd_toEndOf="@+id/imageView81"
        app:layout_constraintStart_toStartOf="@+id/imageView81"
        app:layout_constraintTop_toBottomOf="@+id/textView66" />

    <Button
        android:id="@+id/go_back_to_home"
        android:layout_width="160dp"
        android:layout_height="48dp"
        android:background="@drawable/reset_email_btn"
        android:fontFamily="@font/koho_bold"
        android:onClick="go_back_to_home_btn"
        android:text="Return to Home"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView81"
        app:layout_constraintVertical_bias="0.155"
        tools:ignore="OnClick" />

</androidx.constraintlayout.widget.ConstraintLayout>