<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".Main_Shop_Seller_View_Product">

    <ImageView
        android:id="@+id/imageView59"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="@color/darkbrown" />

    <Button
        android:id="@+id/view_product_seller_back_btn"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/vector"
        app:layout_constraintBottom_toBottomOf="@+id/imageView59"
        app:layout_constraintEnd_toEndOf="@+id/imageView59"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/imageView59"
        app:layout_constraintTop_toTopOf="@+id/imageView59"
        app:layout_constraintVertical_bias="0.705" />

    <ImageView
        android:id="@+id/imageView62"
        android:layout_width="374dp"
        android:layout_height="221dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView59"
        app:layout_constraintVertical_bias="0.034" />

    <ImageView
        android:id="@+id/picture_of_product"
        android:layout_width="227dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView62"
        app:layout_constraintEnd_toEndOf="@+id/imageView62"
        app:layout_constraintHorizontal_bias="0.557"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView62"
        app:layout_constraintVertical_bias="0.523" />

    <ImageView
        android:id="@+id/imageView63"
        android:layout_width="374dp"
        android:layout_height="78dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView62"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView62"
        app:layout_constraintTop_toBottomOf="@+id/imageView62"
        app:layout_constraintVertical_bias="0.038" />

    <TextView
        android:id="@+id/name_of_product_display"
        android:layout_width="300dp"
        android:layout_height="27dp"
        android:fontFamily="@font/koho_medium"
        android:paddingLeft="10dp"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="22dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView63"
        app:layout_constraintEnd_toEndOf="@+id/imageView63"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="@+id/imageView63"
        app:layout_constraintTop_toTopOf="@+id/imageView63"
        app:layout_constraintVertical_bias="0.196" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="45dp"
        android:layout_height="28dp"
        android:fontFamily="@font/jost_medium"
        android:paddingLeft="7dp"
        android:textAlignment="textStart"
        android:textColor="@color/C35314"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView63"
        app:layout_constraintEnd_toEndOf="@+id/name_of_product_display"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/name_of_product_display"
        app:layout_constraintTop_toBottomOf="@+id/name_of_product_display"
        app:layout_constraintVertical_bias="0.538" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="70dp"
        android:layout_height="28dp"
        android:fontFamily="@font/jost_regular"
        android:paddingLeft="7dp"
        android:text="0 sold"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView63"
        app:layout_constraintEnd_toEndOf="@+id/name_of_product_display"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toEndOf="@+id/textView51"
        app:layout_constraintTop_toBottomOf="@+id/name_of_product_display"
        app:layout_constraintVertical_bias="0.538" />

    <Button
        android:id="@+id/menu_product"
        android:layout_width="31dp"
        android:layout_height="28dp"
        android:background="@drawable/dot"
        app:layout_constraintBottom_toBottomOf="@+id/imageView63"
        app:layout_constraintEnd_toEndOf="@+id/imageView63"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toEndOf="@+id/name_of_product_display"
        app:layout_constraintTop_toTopOf="@+id/imageView63"
        app:layout_constraintVertical_bias="0.2" />


    <ImageView
        android:id="@+id/imageView64"
        android:layout_width="374dp"
        android:layout_height="151dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView62"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView62"
        app:layout_constraintTop_toBottomOf="@+id/imageView63"
        app:layout_constraintVertical_bias="0.058" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="65dp"
        android:layout_height="15dp"
        android:fontFamily="@font/jost_regular"
        android:text="Description"
        android:textAlignment="textStart"
        android:textSize="11dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView64"
        app:layout_constraintEnd_toEndOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toStartOf="@+id/imageView64"
        app:layout_constraintTop_toTopOf="@+id/imageView64"
        app:layout_constraintVertical_bias="0.088" />

    <TextView
        android:id="@+id/description_product"
        android:layout_width="341dp"
        android:layout_height="55dp"
        android:fontFamily="@font/jost_regular"
        android:textColor="@color/black"
        android:inputType="textMultiLine"
        android:maxLines="4"
        android:paddingStart="10dp"
        android:paddingTop="10dp"
        android:paddingEnd="10dp"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView64"
        app:layout_constraintEnd_toEndOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.303"
        app:layout_constraintStart_toStartOf="@+id/imageView64"
        app:layout_constraintTop_toBottomOf="@+id/textView54"
        app:layout_constraintVertical_bias="0.135" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="50dp"
        android:layout_height="15dp"
        android:fontFamily="@font/jost_regular"
        android:text="Category"
        android:textAlignment="textStart"
        android:textSize="11dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView64"
        app:layout_constraintEnd_toEndOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="@+id/imageView64"
        app:layout_constraintTop_toBottomOf="@+id/description_product"
        app:layout_constraintVertical_bias="0.217" />

    <TextView
        android:id="@+id/category_of_product"
        android:layout_width="57dp"
        android:layout_height="22dp"
        android:layout_marginBottom="8dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        app:layout_constraintBottom_toBottomOf="@+id/imageView64"
        app:layout_constraintEnd_toEndOf="@+id/imageView64"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="@+id/imageView64" />

    <ImageView
        android:id="@+id/imageView65"
        android:layout_width="374dp"
        android:layout_height="151dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView62"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imageView62"
        app:layout_constraintTop_toBottomOf="@+id/imageView63"
        app:layout_constraintVertical_bias="0.714" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="15dp"
        android:fontFamily="@font/jost_regular"
        android:text="Feedback"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView65"
        app:layout_constraintEnd_toEndOf="@+id/imageView65"
        app:layout_constraintHorizontal_bias="0.026"
        app:layout_constraintStart_toStartOf="@+id/imageView65"
        app:layout_constraintTop_toTopOf="@+id/imageView65"
        app:layout_constraintVertical_bias="0.088" />

</androidx.constraintlayout.widget.ConstraintLayout>