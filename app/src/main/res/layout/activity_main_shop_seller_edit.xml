<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".Main_Shop_Seller_Edit">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@color/darkbrown"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="370dp"
        android:layout_height="180dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.186" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="17dp"
        android:fontFamily="@font/jost_regular"
        android:text="Description"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/profile_picture_shop"
        app:layout_constraintVertical_bias="0.068" />

    <ImageView
        android:id="@+id/name_of_shoppp"
        android:layout_width="251dp"
        android:layout_height="32dp"
        android:background="@drawable/email_reset"
        android:fontFamily="@font/koho_medium"
        android:textSize="15dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.596"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.074" />


    <ImageView
        android:id="@+id/description_of_seller_shoppp"
        android:layout_width="251dp"
        android:layout_height="120dp"
        android:background="@drawable/email_reset"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.596"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.85" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="370dp"
        android:layout_height="35dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.394" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Products"
        android:textAlignment="center"
        android:textColor="@color/FF5C01"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.47"
        tools:ignore="OnClick" />

    <TextView
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Categories"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.47"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/upload_photoo_btn"
        android:layout_width="130dp"
        android:layout_height="39dp"
        android:background="@drawable/background_shop"
        android:drawableLeft="@drawable/upload"
        android:text="Upload Photo"
        android:textSize="10dp"
        android:paddingLeft="20dp"
        android:fontFamily="@font/koho_medium"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.245"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.198" />


    <ImageButton
        android:id="@+id/save_btn_profile_seller"
        android:onClick="save_button_edit_profile_seller"
        android:layout_width="17dp"
        android:layout_height="17dp"
        android:background="@drawable/check_edit_wish"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.162"
        tools:ignore="OnClick" />

    <ImageButton
        android:id="@+id/profile_picture_shop"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/image_upload"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="0.028"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.084" />


    <ImageView
        android:id="@+id/edit_profile_seller_btn"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/cameraa"
        app:layout_constraintBottom_toBottomOf="@+id/profile_picture_shop"
        app:layout_constraintEnd_toEndOf="@+id/profile_picture_shop"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/profile_picture_shop"
        app:layout_constraintTop_toTopOf="@+id/profile_picture_shop"
        android:onClick="edit_profile_seller"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="OnClick" />


    <Button
        android:id="@+id/analytics_btn"
        android:layout_width="130dp"
        android:layout_height="65dp"
        android:background="@color/darkbrown"
        android:drawableTop="@drawable/analytics"
        android:fontFamily="@font/koho_medium"
        android:onClick="analytics"
        android:textColor="@color/white"
        android:text="Analytics"
        android:textSize="15dp"
        android:paddingTop="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="OnClick" />

    <Button
        android:id="@+id/orders_btn"
        android:layout_width="134dp"
        android:layout_height="65dp"
        android:background="@color/light_orange"
        android:drawableLeft="@drawable/orderss"
        android:fontFamily="@font/koho_medium"
        android:onClick="orders"
        android:text="Orders"
        android:textSize="15dp"
        android:paddingLeft="10dp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="OnClick" />


    <Button
        android:id="@+id/list_product_btn"
        android:layout_width="144dp"
        android:layout_height="65dp"
        android:background="@color/green"
        android:drawableLeft="@drawable/list_product"
        android:fontFamily="@font/koho_medium"
        android:onClick="list_product"
        android:text="List Product"
        android:textSize="15dp"
        android:paddingLeft="7dp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="OnClick" />
</androidx.constraintlayout.widget.ConstraintLayout>