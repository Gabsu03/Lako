<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".Main_Shop_Seller_Analytics">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@color/darkbrown"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button4"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/vector"
        android:onClick="my_shop_analytics_btn"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.687"
        tools:ignore="OnClick" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:fontFamily="@font/koho_semibold"
        android:text="Analytics"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.833"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/analytics_graph"
        android:layout_width="374dp"
        android:layout_height="243dp"
        android:background="@drawable/order_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.039" />

    <ImageView
        android:id="@+id/imageView43"
        android:layout_width="117dp"
        android:layout_height="93dp"
        android:textAlignment="center"
        android:background="@drawable/order_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/analytics_graph"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="75dp"
        android:layout_height="19dp"
        android:fontFamily="@font/koho_medium"
        android:text="Total Orders"
        android:textColor="@color/darkbrown"
        app:layout_constraintBottom_toBottomOf="@+id/imageView43"
        app:layout_constraintEnd_toEndOf="@+id/imageView43"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/imageView43"
        app:layout_constraintTop_toTopOf="@+id/imageView43"
        app:layout_constraintVertical_bias="0.157" />

    <TextView
        android:id="@+id/total_orders_analytics"
        android:layout_width="30dp"
        android:layout_height="39dp"
        android:fontFamily="@font/koho_bold"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView43"
        app:layout_constraintEnd_toEndOf="@+id/textView38"
        app:layout_constraintHorizontal_bias="0.422"
        app:layout_constraintStart_toStartOf="@+id/textView38"
        app:layout_constraintTop_toTopOf="@+id/textView38"
        app:layout_constraintVertical_bias="0.38" />


    <ImageView
        android:id="@+id/imageView45"
        android:layout_width="117dp"
        android:layout_height="93dp"
        android:background="@drawable/order_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/analytics_graph"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="100dp"
        android:layout_height="19dp"
        android:fontFamily="@font/koho_medium"
        android:text="Refunded Order"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        app:layout_constraintBottom_toBottomOf="@+id/imageView45"
        app:layout_constraintEnd_toEndOf="@+id/imageView45"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/imageView45"
        app:layout_constraintTop_toTopOf="@+id/imageView45"
        app:layout_constraintVertical_bias="0.157" />

    <TextView
        android:id="@+id/refunded_orders_analytics"
        android:layout_width="30dp"
        android:layout_height="39dp"
        android:fontFamily="@font/koho_bold"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView45"
        app:layout_constraintEnd_toEndOf="@+id/textView42"
        app:layout_constraintStart_toStartOf="@+id/textView42"
        app:layout_constraintTop_toTopOf="@+id/imageView45" />

    <ImageView
        android:id="@+id/imageView46"
        android:layout_width="117dp"
        android:layout_height="93dp"
        android:background="@drawable/order_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.935"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/analytics_graph"
        app:layout_constraintVertical_bias="0.033" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="100dp"
        android:layout_height="19dp"
        android:fontFamily="@font/koho_medium"
        android:text="Total Sales"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        app:layout_constraintBottom_toBottomOf="@+id/imageView46"
        app:layout_constraintEnd_toEndOf="@+id/imageView46"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="@+id/imageView46"
        app:layout_constraintTop_toTopOf="@+id/imageView46"
        app:layout_constraintVertical_bias="0.157" />

    <TextView
        android:id="@+id/total_sales_analytics"
        android:layout_width="30dp"
        android:layout_height="39dp"
        android:fontFamily="@font/koho_bold"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView46"
        app:layout_constraintEnd_toEndOf="@+id/textView43"
        app:layout_constraintStart_toStartOf="@+id/textView43"
        app:layout_constraintTop_toTopOf="@+id/textView43" />
</androidx.constraintlayout.widget.ConstraintLayout>