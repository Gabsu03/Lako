<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_marginBottom="5dp"
    android:layout_height="150dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/imageView60"
        android:layout_width="370dp"
        android:layout_height="30dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.092" />

    <ImageView
        android:id="@+id/imageView72"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:background="@drawable/shopp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView60"
        app:layout_constraintEnd_toEndOf="@+id/imageView60"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/imageView60"
        app:layout_constraintTop_toTopOf="@+id/imageView60"
        app:layout_constraintVertical_bias="0.666" />

    <TextView
        android:id="@+id/cart_name_of_shop"
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="12dp"
        android:text="Shop Name"
        app:layout_constraintBottom_toBottomOf="@+id/imageView60"
        app:layout_constraintEnd_toEndOf="@+id/imageView60"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toEndOf="@+id/imageView72"
        app:layout_constraintTop_toTopOf="@+id/imageView60"
        app:layout_constraintVertical_bias="0.5" />

    <ImageView
        android:id="@+id/imageView73"
        android:layout_width="370dp"
        android:layout_height="86dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView60"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView60"
        app:layout_constraintTop_toBottomOf="@+id/imageView60"
        app:layout_constraintVertical_bias="0.333" />

    <CheckBox
        android:id="@+id/checkbox_cart"
        android:layout_width="27dp"
        android:layout_height="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintEnd_toEndOf="@+id/imageView73"
        app:layout_constraintHorizontal_bias="0.023"
        app:layout_constraintStart_toStartOf="@+id/imageView73"
        app:layout_constraintTop_toTopOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.491" />

    <ImageView
        android:id="@+id/imageView41"
        android:layout_width="111dp"
        android:layout_height="75dp"
        android:background="@drawable/image_cart_bg"
        android:elevation="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintEnd_toEndOf="@+id/imageView73"
        app:layout_constraintHorizontal_bias="0.169"
        app:layout_constraintStart_toStartOf="@+id/imageView73"
        app:layout_constraintTop_toTopOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.5" />

    <ImageView
        android:id="@+id/image_cart"
        android:layout_width="80dp"
        android:layout_height="70dp"
        android:elevation="12dp"
        android:adjustViewBounds="true"
        app:layout_constraintBottom_toBottomOf="@+id/imageView41"
        app:layout_constraintEnd_toEndOf="@+id/imageView41"
        app:layout_constraintStart_toStartOf="@+id/imageView41"
        app:layout_constraintTop_toTopOf="@+id/imageView41" />

    <TextView
        android:id="@+id/cart_name_of_product"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintEnd_toEndOf="@+id/imageView73"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toEndOf="@+id/imageView41"
        app:layout_constraintTop_toTopOf="@+id/imageView73"
        app:layout_constraintVertical_bias="0.136" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="42dp"
        android:layout_height="13dp"
        android:fontFamily="@font/jost_medium"
        android:text="Quantity:"
        android:textAlignment="textStart"
        android:textColor="@color/grey"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/cart_price_of_product"
        app:layout_constraintEnd_toEndOf="@+id/imageView73"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toEndOf="@+id/imageView41"
        app:layout_constraintTop_toBottomOf="@+id/cart_name_of_product"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/quantity_product"
        android:layout_width="70dp"
        android:layout_height="13dp"
        android:fontFamily="@font/jost_regular"
        android:textAlignment="textStart"
        android:textColor="@color/grey"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView67"
        app:layout_constraintEnd_toEndOf="@+id/imageView73"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView67"
        app:layout_constraintTop_toTopOf="@+id/textView67"
        app:layout_constraintVertical_bias="0.538" />

    <TextView
        android:id="@+id/cart_price_of_product"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:fontFamily="@font/jost_medium"
        android:textAlignment="textStart"
        android:textColor="@color/C35314"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintEnd_toEndOf="@+id/imageView73"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toEndOf="@+id/imageView41"
        app:layout_constraintTop_toBottomOf="@+id/cart_name_of_product"
        app:layout_constraintVertical_bias="0.962" />

</androidx.constraintlayout.widget.ConstraintLayout>