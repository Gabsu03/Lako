<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".Profile_My_Shop_Verify_Seller">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@color/darkbrown"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:fontFamily="@font/koho_bold"
        android:text="Verify Shop"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.833"
        tools:ignore="MissingConstraints" />

    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/vector"
        android:onClick="verify_seller_back"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.4"
        tools:ignore="MissingConstraints,OnClick" />


    <TextView
        android:id="@+id/textView30"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Seller Information"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.296" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="370dp"
        android:layout_height="256dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.077" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Name"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="@+id/imageView31"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="@+id/imageView31"
        app:layout_constraintTop_toTopOf="@+id/imageView31"
        app:layout_constraintVertical_bias="0.039" />

    <EditText
        android:id="@+id/input_text_name_verify"
        android:layout_width="338dp"
        android:layout_height="40dp"
        android:background="@drawable/email_reset"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="@+id/imageView31"
        app:layout_constraintStart_toStartOf="@+id/imageView31"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Date of Birth"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="@+id/imageView31"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="@+id/imageView31"
        app:layout_constraintTop_toTopOf="@+id/imageView31"
        app:layout_constraintVertical_bias="0.38" />

    <EditText
        android:id="@+id/input_text_dateofbirth_verify"
        android:layout_width="338dp"
        android:layout_height="40dp"
        android:background="@drawable/email_reset"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="@+id/imageView31"
        app:layout_constraintStart_toStartOf="@+id/imageView31"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="ID Type"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="@+id/imageView31"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="@+id/imageView31"
        app:layout_constraintTop_toTopOf="@+id/imageView31"
        app:layout_constraintVertical_bias="0.73" />

    <EditText
        android:id="@+id/input_text_IDType_verify"
        android:layout_width="338dp"
        android:layout_height="40dp"
        android:background="@drawable/email_reset"
        app:layout_constraintBottom_toBottomOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="@+id/imageView31"
        app:layout_constraintStart_toStartOf="@+id/imageView31"
        app:layout_constraintTop_toBottomOf="@+id/textView33"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/textView34"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Shop Information"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView31"
        app:layout_constraintVertical_bias="0.419" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView31"
        app:layout_constraintVertical_bias="0.09" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Shop Name"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView36"
        app:layout_constraintEnd_toEndOf="@+id/imageView36"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="@+id/imageView36"
        app:layout_constraintTop_toTopOf="@+id/imageView36"
        app:layout_constraintVertical_bias="0.59" />

    <TextView
        android:id="@+id/shop_name_verify_info"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:textColor="@color/orange"
        android:background="@drawable/bar"
        android:fontFamily="@font/koho_bold"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView36"
        app:layout_constraintVertical_bias="0.033" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView31"
        app:layout_constraintVertical_bias="0.386" />

    <TextView
        android:layout_width="170dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Shop Description"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView40"
        app:layout_constraintEnd_toEndOf="@+id/imageView36"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="@+id/imageView40"
        app:layout_constraintTop_toTopOf="@+id/imageView40"
        app:layout_constraintVertical_bias="0.59" />

    <TextView
        android:id="@+id/shop_verify_description_info"
        android:layout_width="370dp"
        android:layout_height="150dp"
        android:background="@drawable/bar"
        android:fontFamily="@font/koho_bold"
        android:textColor="@color/orange"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView40"
        app:layout_constraintVertical_bias="0.13" />

    <Button
        android:id="@+id/create_shop_btn"
        android:onClick="create_shop_btn"
        android:layout_width="160dp"
        android:layout_height="48dp"
        android:background="@drawable/reset_email_btn"
        android:fontFamily="@font/koho_bold"
        android:text="Create Shop"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/shop_verify_description_info"
        app:layout_constraintVertical_bias="0.692"
        tools:ignore="OnClick" />

</androidx.constraintlayout.widget.ConstraintLayout>