<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".Profile_My_Shop_Verify">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:background="@color/darkbrown"
        tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:fontFamily="@font/koho_bold"
        android:text="Verify Shop"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="27dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.833"
        tools:ignore="MissingConstraints" />

    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/vector"
        android:onClick="verify_shop_back"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.4"
        tools:ignore="MissingConstraints,OnClick" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.021" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:background="@drawable/shopp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView30"
        app:layout_constraintEnd_toEndOf="@+id/imageView30"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toStartOf="@+id/imageView30"
        app:layout_constraintTop_toTopOf="@+id/imageView30"
        app:layout_constraintVertical_bias="0.521" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="107dp"
        android:layout_height="27dp"
        android:fontFamily="@font/koho_bold"
        android:text="Shop Name"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView30"
        app:layout_constraintEnd_toEndOf="@+id/imageView30"
        app:layout_constraintHorizontal_bias="0.029"
        app:layout_constraintStart_toEndOf="@+id/imageView31"
        app:layout_constraintTop_toTopOf="@+id/imageView30"
        app:layout_constraintVertical_bias="0.312" />

    <EditText
        android:id="@+id/Shop_name_verify"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:background="@drawable/bar"
        android:fontFamily="@font/koho_regular"
        android:textSize="14dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView30"
        app:layout_constraintVertical_bias="0.017" />

    <EditText
        android:id="@+id/imageView34"
        android:layout_width="370dp"
        android:layout_height="150dp"
        android:background="@drawable/bar"
        android:fontFamily="@font/koho_regular"
        android:textSize="14dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView33"
        app:layout_constraintVertical_bias="0.024" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:layout_marginBottom="440dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Shop_name_verify"
        app:layout_constraintVertical_bias="0.333" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="160dp"
        android:layout_height="27dp"
        android:fontFamily="@font/koho_bold"
        android:text="Shop Description"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView33"
        app:layout_constraintEnd_toEndOf="@+id/imageView33"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/imageView33"
        app:layout_constraintTop_toTopOf="@+id/imageView33"
        app:layout_constraintVertical_bias="0.562" />


    <ImageView
        android:id="@+id/imageView39"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView34" />

    <TextView
        android:layout_width="140dp"
        android:layout_height="27dp"
        android:fontFamily="@font/koho_bold"
        android:text="Shop Location"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView39"
        app:layout_constraintEnd_toEndOf="@+id/imageView39"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/imageView39"
        app:layout_constraintTop_toTopOf="@+id/imageView39"
        app:layout_constraintVertical_bias="0.562" />

    <EditText
        android:id="@+id/Shop_location_verify"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/bar"
        android:fontFamily="@font/koho_regular"
        android:textColor="@color/black"
        android:textSize="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView39" />

    <ImageView
        android:id="@+id/verify_ID"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView34"
        app:layout_constraintVertical_bias="0.37" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="28dp"
        android:fontFamily="@font/koho_bold"
        android:text="Verify Shop using ID"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/verify_ID"
        app:layout_constraintEnd_toEndOf="@+id/verify_ID"
        app:layout_constraintHorizontal_bias="0.033"
        app:layout_constraintStart_toStartOf="@+id/verify_ID"
        app:layout_constraintTop_toTopOf="@+id/verify_ID"
        app:layout_constraintVertical_bias="0.562" />

    <ImageView
        android:id="@+id/verify_ID_whitebg"
        android:layout_width="370dp"
        android:layout_height="100dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/verify_ID"
        app:layout_constraintVertical_bias="0.064" />

    <ImageView
        android:id="@+id/imageView41"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/verify_id"
        app:layout_constraintBottom_toBottomOf="@+id/verify_ID_whitebg"
        app:layout_constraintEnd_toEndOf="@+id/verify_ID_whitebg"
        app:layout_constraintStart_toStartOf="@+id/verify_ID_whitebg"
        app:layout_constraintTop_toTopOf="@+id/verify_ID_whitebg"
        app:layout_constraintVertical_bias="0.75" />

    <Button
        android:id="@+id/verify_shop_btn"
        android:layout_width="160dp"
        android:layout_height="48dp"
        android:background="@drawable/reset_email_btn"
        android:fontFamily="@font/koho_bold"
        android:onClick="continue_verify_shop"
        android:text="Continue"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView41"
        app:layout_constraintVertical_bias="0.396"
        tools:ignore="OnClick" />
</androidx.constraintlayout.widget.ConstraintLayout>