<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="210dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="370dp"
        android:layout_height="25dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059" />

    <ImageView
        android:id="@+id/imageView75"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:background="@drawable/shopp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/name_seller_checkout"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toEndOf="@+id/imageView75"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView76"
        android:layout_width="370dp"
        android:layout_height="86dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.09" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/picture_product_checkout"
        android:layout_width="67dp"
        android:layout_height="59dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView76"
        app:layout_constraintEnd_toEndOf="@+id/imageView76"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="@+id/imageView76"
        app:layout_constraintTop_toTopOf="@+id/imageView76"
        app:layout_constraintVertical_bias="0.518"
        app:shapeAppearanceOverlay="@style/RoundedImage" />

    <TextView
        android:id="@+id/name_product_checkout"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:fontFamily="@font/koho_medium"
        android:textAlignment="textStart"
        android:textColor="@color/darkbrown"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView76"
        app:layout_constraintEnd_toEndOf="@+id/imageView76"
        app:layout_constraintHorizontal_bias="0.055"
        app:layout_constraintStart_toEndOf="@+id/picture_product_checkout"
        app:layout_constraintTop_toTopOf="@+id/imageView76"
        app:layout_constraintVertical_bias="0.212" />

    <TextView
        android:id="@+id/quantity_checkout"
        android:layout_width="200dp"
        android:layout_height="15dp"
        android:fontFamily="@font/jost_medium"
        android:textColor="@color/grey"
        android:textSize="9dp"
        android:textAlignment="textStart"
        app:layout_constraintBottom_toBottomOf="@+id/imageView76"
        app:layout_constraintEnd_toEndOf="@+id/name_product_checkout"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/picture_product_checkout"
        app:layout_constraintTop_toBottomOf="@+id/name_product_checkout"
        app:layout_constraintVertical_bias="0.15" />

    <TextView
        android:id="@+id/price_checkout"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:fontFamily="@font/jost_medium"
        android:textColor="@color/grey"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView76"
        app:layout_constraintEnd_toEndOf="@+id/name_product_checkout"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/picture_product_checkout"
        app:layout_constraintTop_toBottomOf="@+id/quantity_checkout"
        app:layout_constraintVertical_bias="0.44" />

    <ImageView
        android:id="@+id/imageView77"
        android:layout_width="370dp"
        android:layout_height="59dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView76"
        app:layout_constraintVertical_bias="0.529" />

    <ImageView
        android:id="@+id/imageView78"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:background="@drawable/deliver"
