<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".Profile_Settings_Add_Address">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="409dp"
        android:layout_height="200dp"
        android:background="@drawable/lako_header"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="7dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="97dp"
        android:background="@color/darkbrown"
        tools:ignore="MissingConstraints"/>


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="370dp"
        android:layout_height="35dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.015" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="79dp"
        android:layout_height="30dp"
        android:fontFamily="@font/koho_bold"
        android:text="Address"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toEndOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.538"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/address"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintHorizontal_bias="0.376"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.538" />


    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="370dp"
        android:layout_height="540dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="3" />

    <TextView
        android:id="@+id/nameTextView"
        android:layout_width="48dp"
        android:layout_height="27dp"
        android:fontFamily="@font/jost_semibold"
        android:text="Name"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.012"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_name_text"
        android:layout_width="321dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_semibold"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.048">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/nameInput"
            android:layout_width="321dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_semibold"
            android:inputType="text"
            android:paddingLeft="10dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/phoneTextView"
        android:layout_width="48dp"
        android:layout_height="27dp"
        android:fontFamily="@font/jost_semibold"
        android:text="Label"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.173"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_label_text"
        android:layout_width="321dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_medium"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.228">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/labelInput"
            android:layout_width="321dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_regular"
            android:inputType="text"
            android:paddingLeft="10dp" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_phone_text"
        android:layout_width="321dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_medium"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.4">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/phoneInput"
            android:layout_width="321dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_regular"
            android:inputType="phone"
            android:paddingLeft="10dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:layout_width="114dp"
        android:layout_height="25dp"
        android:fontFamily="@font/jost_semibold"
        android:text="Phone Number"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.097"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.321"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="50dp"
        android:layout_height="20dp"
        android:fontFamily="@font/jost_semibold"
        android:text="Region"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.474"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_region_text"
        android:layout_width="321dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_medium"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.562">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/regionInput"
            android:layout_width="321dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_regular"
            android:inputType="text"
            android:paddingLeft="10dp" />


    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:fontFamily="@font/jost_semibold"
        android:text="Street Name"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toTopOf="@+id/address_street_number_text"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.703"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toBottomOf="@+id/address_region_text"
        app:layout_constraintVertical_bias="0.727"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="110dp"
        android:layout_height="20dp"
        android:fontFamily="@font/jost_semibold"
        android:text="House Number"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.096"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.623"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_house_number_text"
        android:layout_width="149dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_medium"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.72">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/houseNumberInput"
            android:layout_width="149dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_regular"
            android:inputType="number"
            android:paddingLeft="10dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_street_number_text"
        android:layout_width="149dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_medium"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.891"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.72">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/streetInput"
            android:layout_width="149dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_regular"
            android:inputType="text"
            android:paddingLeft="10dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:layout_width="28dp"
        android:layout_height="20dp"
        android:fontFamily="@font/jost_semibold"
        android:text="City"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.073"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.767"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_City_text"
        android:layout_width="149dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_medium"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.884">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/cityInput"
            android:layout_width="149dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_regular"
            android:inputType="text"
            android:paddingLeft="10dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:fontFamily="@font/jost_semibold"
        android:text="Zip Code"
        android:textAlignment="textStart"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.656"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.767"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/address_Zipcode_text"
        android:layout_width="149dp"
        android:layout_height="70dp"
        android:fontFamily="@font/jost_medium"
        android:textColorHint="@color/white"
        app:hintTextColor="@color/C35314"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.891"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22"
        app:layout_constraintVertical_bias="0.884">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/zipcodeInput"
            android:layout_width="149dp"
            android:layout_height="40dp"
            android:background="@drawable/email_reset"
            android:fontFamily="@font/jost_regular"
            android:inputType="number"
            android:paddingLeft="10dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/address_save_btn"
        android:layout_width="160dp"
        android:layout_height="48dp"
        android:background="@drawable/reset_email_btn"
        android:fontFamily="@font/koho_bold"
        android:text="Save"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/imageView22"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toBottomOf="@+id/address_region_text"
        app:layout_constraintVertical_bias="0.906" />

    <ProgressBar
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_margin="16dp"
        android:layout_marginBottom="16dp"
        android:elevation="7dp"
        android:visibility="gone"
        android:indeterminateTint="@color/darkbrown"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/address_save_btn"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="@+id/address_save_btn"
        app:layout_constraintTop_toBottomOf="@+id/address_save_btn"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>