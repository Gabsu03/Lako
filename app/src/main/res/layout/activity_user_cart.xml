<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary"
    tools:context=".User_Cart">

    <ImageView
        android:id="@+id/imageView66"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="@color/darkbrown" />

    <Button
        android:layout_width="12dp"
        android:layout_height="20dp"
        android:background="@drawable/vector"
        android:onClick="cart_back_button"
        app:layout_constraintBottom_toBottomOf="@+id/imageView66"
        app:layout_constraintEnd_toStartOf="@+id/textView57"
        app:layout_constraintHorizontal_bias="0.172"
        app:layout_constraintStart_toStartOf="@+id/imageView66"
        app:layout_constraintTop_toTopOf="@+id/imageView66"
        app:layout_constraintVertical_bias="0.744"
        tools:ignore="OnClick" />

    <TextView
        android:id="@+id/textView57"
        android:layout_width="200dp"
        android:layout_height="27dp"
        android:fontFamily="@font/koho_bold"
        android:text="Checkout"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView66"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView66"
        app:layout_constraintTop_toTopOf="@+id/imageView66"
        app:layout_constraintVertical_bias="0.722" />

    <ImageView
        android:id="@+id/imageView67"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:background="@drawable/bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView66"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="22dp"
        android:fontFamily="@font/koho_bold"
        android:text="My Shopping Cart"
        android:textAlignment="center"
        android:textColor="@color/darkbrown"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView67"
        app:layout_constraintEnd_toEndOf="@+id/imageView67"
        app:layout_constraintStart_toStartOf="@+id/imageView67"
        app:layout_constraintTop_toTopOf="@+id/imageView67" />

</androidx.constraintlayout.widget.ConstraintLayout>